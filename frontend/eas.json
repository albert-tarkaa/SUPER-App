{
  "cli": {
    "version": ">= 10.2.2"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "apk": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
       "EXPO_PUBLIC_OPENROUTE_API_KEY": "${EXPO_PUBLIC_OPENROUTE_API_KEY}",
        "EXPO_PUBLIC_OPENWEATHER_API_KEY": "${EXPO_PUBLIC_OPENWEATHER_API_KEY}",
        "EXPO_PUBLIC_VOICE_API_KEY": "${EXPO_PUBLIC_VOICE_API_KEY}",
        "EXPO_PUBLIC_AIR_QUALITY_OPEN_DATA_PLATFORM_API_KEY": "${EXPO_PUBLIC_AIR_QUALITY_OPEN_DATA_PLATFORM_API_KEY}",
        "EXPO_PUBLIC_PREDICTHQ_API_KEY": "${EXPO_PUBLIC_PREDICTHQ_API_KEY}",
        "EXPO_PUBLIC_API_BASE_URL": "${EXPO_PUBLIC_API_BASE_URL}"
      }
    },
    "production": {
      "env": {
        "EXPO_PUBLIC_OPENROUTE_API_KEY": "${EXPO_PUBLIC_OPENROUTE_API_KEY}",
        "EXPO_PUBLIC_OPENWEATHER_API_KEY": "${EXPO_PUBLIC_OPENWEATHER_API_KEY}",
        "EXPO_PUBLIC_VOICE_API_KEY": "${EXPO_PUBLIC_VOICE_API_KEY}",
        "EXPO_PUBLIC_AIR_QUALITY_OPEN_DATA_PLATFORM_API_KEY": "${EXPO_PUBLIC_AIR_QUALITY_OPEN_DATA_PLATFORM_API_KEY}",
        "EXPO_PUBLIC_PREDICTHQ_API_KEY": "${EXPO_PUBLIC_PREDICTHQ_API_KEY}",
        "EXPO_PUBLIC_API_BASE_URL": "${EXPO_PUBLIC_API_BASE_URL}"
      }
    }
  },
  "submit": {
    "production": {}
  }
}